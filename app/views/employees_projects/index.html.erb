<h1>Listing employees_projects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employees_projects.each do |employees_project| %>
      <tr>
        <td><%= link_to 'Show', employees_project %></td>
        <td><%= link_to 'Edit', edit_employees_project_path(employees_project) %></td>
        <td><%= link_to 'Destroy', employees_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

