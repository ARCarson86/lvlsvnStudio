<h1>Listing employees_skills</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employees_skills.each do |employees_skill| %>
      <tr>
        <td><%= link_to 'Show', employees_skill %></td>
        <td><%= link_to 'Edit', edit_employees_skill_path(employees_skill) %></td>
        <td><%= link_to 'Destroy', employees_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

